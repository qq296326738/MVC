}
.luc_button:before{
	position:absolute;
	top:0;
	left:0;
	right:0;
	bottom:0;
	margin-top:-4px;
	margin-left:-4px;
	z-index:-2;
	width:68px;
	height:58px;
	border:solid 4px rgba(0,0,0,0.4);
	border-radius:50%;
	content:"";
}
.luc_button:after{
	position:absolute;
	z-index:-2;
	left:20px;
	bottom:0px;
	width:30px;
	height:0px;
