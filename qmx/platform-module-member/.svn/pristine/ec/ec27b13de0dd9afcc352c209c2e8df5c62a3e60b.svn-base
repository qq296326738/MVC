}
.ind_nav_icon.ind_nav_ico_tick{
	background:url(../images/index_nav.png) no-repeat scroll #0aac01 9px 6px/30px 175px;
}
.ind_nav_icon.ind_nav_ico_hotel{
	background:url(../images/index_nav.png) no-repeat scroll #ff7d29 9px -40px/30px 175px;
}
.ind_nav_icon.ind_nav_ico_free{
	background:url(../images/index_nav.png) no-repeat scroll #5686f2 9px -87px/30px 175px;
}
.ind_nav_icon.ind_nav_ico_hot{
	background:url(../images/index_nav.png) no-repeat scroll #dd127b 9px -135px/30px 175px;
}
.ind_module{
	margin-top:14px;
	background-color:#fff;	
}
		/*轮播start*/
.slide {
    overflow: hidden;
    position: relative;
    width: 100%;
}
.slide .slide-imgul {
    width: 10000px;
}
.slide .slide-imgul li {
    float: left;
}
.slide .slide-imgul li a {
    display: block;
}
.slide .slide-imgul li img {
    width: 100%;
	display:block;
}
.slide .slide-control {
	width:100%;
    bottom: 5px;
    position: absolute;
    text-align:center;
}
.conter-ul li {
}
.slide .slide-control .conter-ul{
	width:100%;	
}
.slide .slide-control .conter-ul li {
	display:inline-block;
    border: 2px solid rgba(255, 255, 255, 0.8);
    border-radius: 50%;
    height: 4px;
    width: 4px;
}
.slide .slide-control .conter-ul li.picth {
    background-color: #fff;
}
		/*轮播end*/
.ind_title{
	height:32px;
}
.ind_title h2{
	font-size:1em;
	font-weight:100;
	margin-left:10px;
	display:inline-block;
	margin-top:10px;
}
.ind_title .more{
	float:right;
	font-size:0.8em;
	margin-right:5px;
	margin-top:13px;
}
.ind_mod_spot .ind_title h2{
	border-bottom:solid 2px #0aac01;	
}
.ind_mod_hot .ind_title h2{
	border-bottom:solid 2px #ff7d29;
}
.ind_mod_spot .ind_title .more{
	color:#0aac01;
}
.ind_tit_icon{
