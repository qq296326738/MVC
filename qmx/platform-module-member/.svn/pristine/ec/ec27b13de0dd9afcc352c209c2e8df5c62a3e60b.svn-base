.luc_box{
	width:100%;
	height:100px;
	position:relative;	
}
.luc_box_border{
	position:relative;
	padding:20px;
	padding-right:13px;
	background:#fffab5;
	border:solid 2px #54a5cb;
	border-radius:10px;
	box-sizing:border-box;
	z-index:1;
}
.luc_box_border:before{
	position:absolute;
	top:0px;
	left:0px;
	right:0px;
	bottom:0px;
	z-index:-1;
	content:"";
	border:solid 10px #e80381;	
	border-radius:8px;
}
.luc_box_border:after{
	position:absolute;
	top:10px;
	left:10px;
	right:10px;
	bottom:10px;
	z-index:-1;
	content:"";
	border:solid 3px #fff;
	border-radius:5px;
	box-shadow:0 0 5px 1px #ccc inset; 
}
.luc_box_ul{
	height:auto;
	overflow:hidden;
	color:#fff;
	margin-bottom:7px;
}
.luc_box_li{
	width:33.33%;
	float:left;
	text-align:center;
	padding-right:7px;
	box-sizing:border-box;
	margin-bottom:7px;
}
.luc_box_li:nth-child(2n-1) .luc_pri_box{
	background-color:#e80381;
}
.luc_box_li:nth-child(2n) .luc_pri_box{
	background-color:#57beeb;
}
.luc_box_li.picth .luc_pri_box{
	background-color:#f60;	
}
.luc_pri_box{
	position:relative;
	width:100%;
	height:0;
	padding-top:72%;	
}
.luc_li_prize{
	text-align:center;
	height:46px;
