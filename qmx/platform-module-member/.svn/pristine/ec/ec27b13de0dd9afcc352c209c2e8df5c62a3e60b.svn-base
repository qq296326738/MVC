}
.luc_button{
	position:relative;
	font-size:1em;
	color:#fff;
	line-height:1;
	display:inline-block;
	padding:13px 18px;
	margin:33px auto 20px auto;
	background-color:#e80381;
	border-radius:50%;
	box-shadow:0 0 5px 2px rgba(0,0,0,0.3) inset;
	text-shadow:1px 1px 2px rgba(0,0,0,0.6);
