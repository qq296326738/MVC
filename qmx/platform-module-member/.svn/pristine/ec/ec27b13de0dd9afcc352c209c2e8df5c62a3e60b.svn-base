@charset "utf-8";

/* 公用 */
*{
	margin:0;
	padding:0;
}
a,a:link,a:visited,a:active,a:hover{
	text-decoration:none;	
}
ul,li{
	list-style:none;
}
input,textarea{
	border:0;
	-webkit-appearance:none;	
}
.wap_box{
	width:100%;
	margin:0 auto;	
}
.logo{
	width:74px;	
}
.width-1, .width-2, .width-3, .width-4, .width-5, .width-6, .width-7, .width-8, .width-9, .width-10, .width-11, .width-12 {
    box-sizing: border-box;
    float: left;
}
.width-1 {
    width: 8.333%;
}
.width-2 {
    width: 16.666%;
}
.width-3 {
    width: 25%;
}
.width-4 {
    width: 33.333%;
}
.width-5 {
    width: 41.666%;
}
.width-6 {
    width: 50%;
}
.width-7 {
    width: 58.333%;
}
.width-8 {
    width: 66.666%;
}
.width-9 {
    width: 75%;
}
.width-10 {
    width: 83.333%;
}
.width-11 {
    width: 91.666%;
}
.width-12 {
    width: 100%;
}

/*按钮*/
.button_1,.button_2{
	display:inline-block;
	width:44px;
	height:20px;
	text-align:center;
	line-height:20px;
	font-size:0.75em;
	border-radius:2px;
}
.button_1{
	color:#fff;
	background-color:rgba(0,0,0,0.2);
	border:solid 1px transparent;
}
.button_2{
	color:#dd127b;
	border:solid 1px #dd127b;
}
.button_3{
	padding:2px 7px;
	color:#fff;
	background-color:#dd127b;
	border:solid 1px #dd127b;
	border-radius:3px;
	font-size:0.8em;		
}
.button_4{
	padding:2px 7px;
	color:#fff;
	background-color:#ff9000;
	border:solid 1px #ff9000;
	border-radius:3px;
	font-size:0.8em;		
}
.button_5{
	padding:2px 7px;
	color:#fff;
	background-color:#0aad00;
	border:solid 1px #0aad00;
	border-radius:3px;
	font-size:0.8em;	
}
.button_6{
	padding:2px 7px;
	color:#fff;
	background-color:#5686f2;
	border:solid 1px #5686f2;
	border-radius:3px;
	font-size:0.8em;
}
.button_7{
	padding:2px 7px;
	color:#0aad00;
	background-color:#fff;
	border:solid 1px #0aad00;
	border-radius:3px;
	font-size:0.8em;
}
.overhide{
	height:auto;
	overflow:hidden;
}

/* 主页 */
.ind_top{
	width:100%;
	position:relative;
}
.ind_top .ind_top_bg{
	width:100%;
}
.ind_top .ind_top_box{
	width:100%;
