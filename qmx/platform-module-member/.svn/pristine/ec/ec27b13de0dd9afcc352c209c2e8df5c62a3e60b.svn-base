	width:18px;
	height:18px;
	vertical-align:-3px;
	margin-right:5px;
}
.ind_tit_icon.ind_tit_ico_spot{
	background:url(../images/index_icon.png) no-repeat scroll 0 -27px/18px 73px;
}
.ind_tit_icon.ind_tit_ico_hot{
