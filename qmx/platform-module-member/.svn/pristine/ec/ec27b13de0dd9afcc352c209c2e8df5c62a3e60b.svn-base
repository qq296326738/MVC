	font-size:1em;	
}
.luc_li_prize p{
	font-size:0.8em;
}
.luc_box_border .fg_red_2{
	font-size:0.8em;
	padding-right:7px;
	line-height:1.5;
	margin-top:3px;
}
.luc_box_border .fg_blue_1{
	font-size:0.9em;
	padding-right:7px;
	line-height:1.2;
	margin-top:2px;	
}
.luc_center{
