}
.ind_mod_hot .ind_lis_box{
	padding:0 10px;	
}
.ind_hot_sort{
	overflow:hidden;
	padding:16px 0;
}
.ind_hot_sort li{
	float:left;
	margin-right:5px;
}
.ind_hot_sort li a{
	display:block;
