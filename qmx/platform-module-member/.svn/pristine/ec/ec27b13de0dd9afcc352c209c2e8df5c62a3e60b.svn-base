	position:absolute;
	right:3px;
}
.ind_top .ind_top_search .ind_top_sea_center{
	width:100%;
	box-sizing:border-box;
	padding-left:75px;
	padding-right:27px;
	position:relative;
}
.ind_top_nav{
	background-color:#fff;
	padding:0 5%;
}
.ind_top_nav_ul_li{
	
}
.ind_top_nav_ul_li a{
	display:block;
	padding-top:8px;
	padding-bottom:13px;
}
.ind_top_nav_ul_li p{
	text-align:center;
	margin-top:8px;
	color:#676767;
	font-size:0.8em;	
}
.ind_nav_icon{
	display:block;
	width:48px;
	height:48px;
	border:solid 1px transparent;
	border-radius:50%;
