package com.qmx.member.enumerate;

/**
 * 充值消费方式
 */
public enum RechargeType {
    wx("微信"),
    zfb("支付宝"),
    hyye("会员余额"),
    huodong("活动"),
    duihuan("兑换"),
    xxhyye("线下会员余额"),
    xxwx("线下微信"),
    xxzfb("线下支付宝"),
    xxxinajin("线下现金"),
    xxyinhangka("线下银行卡"),
    xxqita("线下其他"),
    xxduihuan("线下兑换");

    private String title;

    RechargeType(String title) {
        this.title = title;
    }

    public String getTitle() {
        return title;
    }

    public void setTitle(String title) {
        this.title = title;
    }
}
