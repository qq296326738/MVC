/*公共*/
html, body {
	width: 100%;
	/*height: 100%;*/
}
ul {
	list-style: none;
}
a, a:active {
	color: initial;
}
h1, h2, h3, h4, h5, h6 {
	margin-top: 0;
	margin-bottom: 0;
}
p {
	margin-bottom: 0;
}
.member {
	position: relative;
	max-width: 500px;
	margin: 0 auto;
}
.member-table {
	display: table;
}
.member-table-cell {
	display: table-cell;
}
.member-table-cell a {
	display: block;
	width: 100%;
	height: 100%;
}
.member-flex {
	display: flex;
}
.member-flex .member-flex-main {
	flex-grow: 1;
}
.member-flex-item {
	
}
.member-title {
	position: relative;
	font-size: 15px;
	margin-top: 10px;
	background-color: #fff;
	text-align: center;
	padding-top: 10px;
	padding-bottom: 10px;
	line-height: 1.5;
}
.member-title-link {
	position: absolute;
	font-size: 12px;
	font-weight: 400;
	right: 15px;
	top: 50%;
	transform: translateY(-50%);
}
.member-title-link:after {
	position: absolute;
	content: "";
	top: -5px;
	bottom: -5px;
	left: -5px;
	right: -5px;
}
.member-row {
	background-color: #fff;
	padding: 10px 15px;
}
.mui-content.full-white {
	width: 100%;
	min-height: 100%;
	background-color: #fff;
}

/*前景色*/
.fg-gray {
	color: #c8c8c8 !important;
}
.fg-blue {
	color: #0072ff !important;
}
.fg-black {
	color: #000 !important;
}
.fg-orange {
	color: #f98d02 !important;
}

/*按钮*/
.member-btn-1, .member-btn-2 {
	display: block;
	margin-left: 30px;
	margin-right: 30px;
	padding-top: 12px;
	padding-bottom: 12px;
	font-size: 15px;
	line-height: 1;
	border: solid 1px #0072ff;
	border-radius: 10px;
	text-align: center;
}
.member-btn-1 {
	background-color: #0072ff;
	color: #fff;
}
.member-btn-2 {
	color: #0072FF;
}
.member-btn-3 {
	display: block;
	width: 60%;
	margin-left: auto;
	margin-right: auto;
	padding-top: 10px;
	padding-bottom: 10px;
	font-size: 14px;
	line-height: 1;
	color: #fff;
	border: solid 1px #0072ff;
	background-color: #0072ff;
	border-radius: 5px;
	text-align: center;
}
.member-btn-4 {
	display: inline-block;
	color: #fff;
	background-color: #e3164f;
	border: solid 1px #e3164f;
	border-radius: 4px;
	font-size: 12px;
	padding: 3px 7px;
	line-height: 1;
}
.member-btn-5 {
	display: inline-block;
	color: #fff;
	background-color: #f98d02;
	border: solid 1px #f98d02;
	border-radius: 5px;
	font-size: 14px;
	padding: 8px 40px;
	line-height: 1;
}

/*选择器*/
.member-picker-1 {
	position: relative;
	padding: 8px 15px;
	background: #fff;
	font-size: 14px;
}
.member-picker-1:after {
	position: absolute;
	content: "";
	width: 0;
	height: 0;
	box-sizing: content-box;
	border-top: solid 8px #000;
	border-left: solid 5px transparent;
	border-right: solid 5px transparent;
	right: 15px;
	top: 50%;
	transform: translateY(-50%);
}

/*文本框*/
input[type="text"].member-input-1 {
	border: solid 1px #000;
	border-radius: 10px;
	padding: 5px 10px;
	font-size: 14px;
	height: auto;
	margin-bottom: 0;
}

/*首页*/
.index-header {
	background-color: #f98d02;
	padding: 15px 15px 35px 15px;
	color: #fff;
	box-shadow: 0 3px 7px rgba(121,75,4,.45);
}
.header-img {
	width: 80px;
	height: 80px;
	border-radius: 50%;
	margin: 0 auto;
	overflow: hidden;
}
.header-img > img {
	display: block;
	width: 100%;
}
.index-header .mui-text-center {
	font-size: 12px;
	margin-top: 8px;
}
.index-header .icon {
	display: inline-block;
	width: 13px;
	height: 13px;
	vertical-align: -3px;
	margin-right: 4px;
}
.index-header .icon.icon-wallet {
	background: url(../images/index-icon.png) no-repeat scroll 0 -200px / 21px 231px;
}
.index-header .icon.icon-gift {
	background: url(../images/index-icon.png) no-repeat scroll 0 -218px / 21px 231px;
}
.index-header .member-table {
	position: relative;
	background-color: #fff;
	color: #f98d02;
	width: 85%;
	margin: 10px auto -55px auto;
	padding: 10px;
	font-size: 10px;
	box-shadow: 0 2px 4px rgba(182,95,1,.35);
}
.index-header .member-table:after {
	position: absolute;
	content: "";
	width: 1px;
	height: 50%;
	left: 50%;
	top: 50%;
	transform: translate(-50%, -50%);
	background-color: #f98d02;
}
.index-header .member-table-cell {
	width: 50%;
	text-align: center;
}
.index .mui-content {
	margin-top: 30px;
	background-color: #fff;
}
.member-link-row {
	font-size: 15px;
	padding: 10px 20px;
}
.mui-content a > .member-link-row {
	border-bottom: solid 1px #c8c8c8;
}
.mui-content a:last-child > .member-link-row {
	border-bottom: none;
}
.member-link-row .mui-icon-arrowright {
	font-size: 21px;
	color: #c8c8c8;
}
.member-link-row .icon {
	display: inline-block;
	width: 23px;
	height: 20px;
	vertical-align: -5px;
	margin-right: 5px;
}
.member-link-row .icon.icon-user {
	background: url(../images/index-icon.png) no-repeat scroll 0 0 / 21px 231px;
}
.member-link-row .icon.icon-sign {
	background: url(../images/index-icon.png) no-repeat scroll 0 -25px / 21px 231px;
}
.member-link-row .icon.icon-recharge {
	background: url(../images/index-icon.png) no-repeat scroll 0 -50px / 21px 231px;
}
.member-link-row .icon.icon-card {
	background: url(../images/index-icon.png) no-repeat scroll 0 -75px / 21px 231px;
}
.member-link-row .icon.icon-consume {
	background: url(../images/index-icon.png) no-repeat scroll 0 -100px / 21px 231px;
}
.member-link-row .icon.icon-code {
	background: url(../images/index-icon.png) no-repeat scroll 0 -125px / 21px 231px;
}
.member-link-row .icon.icon-discount {
	background: url(../images/index-icon.png) no-repeat scroll 0 -150px / 21px 231px;
}
.member-link-row .icon.icon-phone {
	background: url(../images/index-icon.png) no-repeat scroll 0 -175px / 21px 231px;
}

/*充值记录*/
.record .member-flex-main {
	padding-right: 15px;
}
.member-part {
	padding-top: 10px;
}
.member-part-title {
	padding: 5px 15px;
}
.member-part-title, .member-part h4, .member-part h5 {
	font-size: 14px;
	font-weight: 400;
	color: #000;
	line-height: 1.5;
}
.member-flex-main + div {
	flex-shrink: 0;
}
.member-part .member-row div:last-child {
	text-align: right;
	/**/
} 
.member-part p {
	font-size: 12px;
	color: #969696;
}
.member-part .member-row {
	border-bottom: solid 1px #c8c8c8;
}
.member-row.member-border-none {
	border-bottom: none;
}
.member-part-label {
	flex-shrink: 0;
}

/*详情*/
/*.details .member-flex-main {
	flex-grow: 1;
}*/

/*兑换成功*/
.pay-state {
	text-align: center;
	padding-top: 40px;
	padding-bottom: 40px;
}
.pay-state, .pay-state p {
	color: #fff;
}
.pay-state p {
	font-size: 12px;
}
.pay-state.success {
	background-color: #0072ff;
}
.pay-state.fail {
	background-color: #969696;
}
.pay-state .member-flex {
	display: inline-block;
	margin: 0 auto;
}
.pay-state .member-flex > div {
	text-align: left;
	display: inline-block;
}
.pay-state .icon {
	display: inline-block;
	width: 62px;
	height: 62px;
	margin-right: 20px;
	vertical-align: bottom;
}
.pay-state .icon.icon-wallet {
	background: url(../images/pay-state-icon.png) no-repeat scroll 0 0 / 62px 62px;
}
.pay-state h3 {
	margin-bottom: 10px;
	font-size: 20px;
}

/*消费记录，积分记录*/
.list .member-flex {
	align-items: center;
}

/*余额*/
.balance .pay-state .member-flex > div {
	text-align: center;
}
.balance .member-table {
	width: 100%;
	text-align: center;
	font-size: 10px;
	color: #0072ff;
	border-top: solid 1px #0072ff;
	border-bottom: solid 1px #0072ff;
}
.balance .member-table-cell {
	width: 50%;
	padding-top: 5px;
	padding-bottom: 5px;
}
.balance .member-table-cell:first-child {
	border-right: solid 1px #0072ff;
}
.balance .member-btn-1 {
	margin-top: 35px;
}
.balance .member-btn-2 {
	margin-top: 15px;
}

/*充值*/
.recharge .member-title, .recharge .member-title-link {
	color: #2073fb;
}
.recharge .member-part h4, .recharge .member-part .member-input-1 {
	text-align: left;
	margin-bottom: 3px;
}
.recharge .member-btn-3 {
	margin-top: 90px;
}

/*支付方式*/
.pay .member {
	background-color: #fff;
	height: 100%;
}
.pay .mui-content:last-child {
	background-color: #fff;
	border-top: solid 10px #f5f5fa;
}
.pay .mui-content:last-child .member-part {
	padding-top: 0;
}
.pay .member-row {
	position: relative;
}
.pay .mui-content:first-child h4, .pay .mui-content:first-child  p {
	font-size: 17px;
}
.pay .mui-content:last-child h4, .pay .mui-content:last-child  p {
	font-size: 15px;
}
.pay h4.mui-text-center {
	color: #6e6e6e;
}
.pay input[type='radio'] {
	position: absolute;
	width: 100%;
	height: 100%;
	left: 0;
	top: 0;
	opacity: 0;
}
.pay .member-flex {
	align-items: center;
}
.pay .member-part-label .icon {
	display: inline-block;
	width: 32px;
	height: 32px;
	vertical-align: -10px;
	margin-right: 10px;
}
.pay .member-part-label .icon.icon-weixin {
	background: url(../images/pay-icon.png) no-repeat scroll 0 0 / 32px 113px;
}
.pay .member-part-label .icon.icon-zhifubao {
	background: url(../images/pay-icon.png) no-repeat scroll 0 -37px / 32px 113px;
}
.pay .member-flex-main .icon {
	display: inline-block;
	width: 17px;
	height: 17px;
	margin-top: 5px;
}
.pay .member-flex-main .icon.icon-radio {
	background: url(../images/pay-icon.png) no-repeat scroll 0 -74px / 32px 113px;
}
.pay input[type='radio']:checked + .member-flex-main .icon.icon-radio {
	background: url(../images/pay-icon.png) no-repeat scroll 0 -96px / 32px 113px;
}
.pay .member-btn-1 {
	margin-left: 15px;
	margin-right: 15px;
	margin-top: 90px;
}

/*兑换商城*/
.exchange-header {
	padding: 10px 15px;
	text-align: center;
	background: #0072ff;
}
.exchange-header * {
	color: #fff;
	line-height: 1;
}
.exchange-header > * {
	margin-top: 10px;
}
.exchange-header h1, .exchange-header h3 {
	font-weight: 400;
}
.exchange-header h1 {
	font-size: 50px;
}
.exchange-header h3 {
	font-size: 15px;
}
.exchange-header .member-table {
	position: relative;
	margin-left: auto;
	margin-right: auto;
	font-size: 12px;
	border: solid 1px #fff;
	border-radius: 5px;
}
.exchange-header .member-table:after {
	position: absolute;
	content: "";
	height: 100%;
	width: 1px;
	background-color: #fff;
	top: 0;
	left: 50%;
	transform: translateX(-50%);
}
.exchange-header .member-table-cell {
	padding: 3px 15px;
}
.exchange-header p {
	font-size: 11px;
}
.exchange .mui-slider-indicator {
	bottom: 5px;
}
.exchange .mui-slider-indicator .mui-indicator {
	box-shadow: none;
	margin-left: 3px;
	margin-right: 3px;
}
.member-shop-list {
	padding-left: 5px;
	padding-right: 5px;
	background: #ddf5ff;
}
.member-shop-box {
	padding: 5px;
	background-color: #fff;
}
.member-shop-list .member-flex {
	width: 100%;
	background-color: #ddf5ff;
	padding: 0 6px 6px 0;
	align-items: stretch;
}
.member-shop-list .member-flex:first-child {
	padding-top: 5px;
}
.member-shop-list .member-flex-item {
	width: 33.3333%;
	text-align: center;
	padding-left: 6px;
}
.member-shop-obj {
	position: relative;
	background-color: #fff;
	height: 100%;
	padding-bottom: 50px;
}
.member-shop-list .member-flex-item h4 {
	font-size: 12px;
	color: #888689;
	margin-top: 3px;
	padding-left: 5px;
	padding-right: 5px;
}
.member-shop-list .member-flex-item h3 {
	font-size: 14px;
	color: #182c4a;
	margin-top: 3px;
	position: absolute;
	width: 100%;
	bottom: 33px;
}
.member-shop-list .member-btn-4 {
	position: absolute;
	bottom: 8px;
	left: 50%;
	transform: translateX(-50%);
	white-space: nowrap;
}
.member-shop-img {
	position: relative;
	width: 100%;
	height: 0;
	padding-top: 80%;
	overflow: hidden;
}
.member-shop-img > img {
	position: absolute;
	width: 100%;
	top: 50%;
	left: 50%;
	transform: translate(-50%, -50%);
}

/*兑换地址*/
.exchange-address .mui-content {
	background: #fff;
	min-height: 100%;
}
.exchange-address .member-flex {
	align-items: center;
}
.exchange-address input {
	margin-bottom: 0;
	padding: 0;
	height: auto;
	font-size: 12px;
	border: none;
}
.exchange-address .member-part {
	padding-top: 0;
	background-color: #fff;
	border-top: solid 10px #efeff4;
}
.exchange-address .member-row {
	padding-left: 10px;
	padding-right: 10px;
	margin-left: 15px;
	margin-right: 15px;
	border-color: #000;
}
.member-number-picker {
	display: inline-block;
}
.member-number-picker .fa, .member-number-picker .member-number-input {
	display: inline-block;
	float: left;
	height: 16px;
	line-height: 16px;
}
.member-number-picker .fa {
	position: relative;
	text-align: center;
	border: solid 1px #aaaaaa;
	font-weight: 100;
	width: 16px;
	font-size: 10px;
	color: #c8c8c8;
}
.member-number-picker .fa:after {
	position: absolute;
	content: "";
	top: -10px;
	left: -10px;
	right: -10px;
	bottom: -10px;
}
.member-number-picker .fa:first-child:after {
	left: -20px;
	right: 0;
}
.member-number-picker .fa:last-child:after {
	left: 0;
	right: -20px;
}
.member-number-picker .member-number-input {
	width: 36px;
	border-radius: 0;
	border-top: solid 1px #aaa;
	border-bottom: solid 1px #aaa;
	text-align: center;
}
.member-shop-explain {
	padding: 10px 20px 0 20px;
	background-color: #efeff4;
}
.member-explain-img {
	width: 160px;
}
.member-explain-img > img {
	width: 100%;
}
.member-explain-text {
	padding-left: 10px;
}
.member-explain-text h4 {
	font-size: 12px;
	font-weight: 400;
	margin-bottom: 5px;
}
.member-explain-text p {
	font-size: 10px;
	color: #000;
}
.exchange-address .member-btn-3 {
	margin-top: 35px;
	width: 50%;
}

/*优惠券*/
.coupon .mui-content {
	position: relative;
	padding: 63px 15px 42px 15px;
	height: 100%;
}
.coupon .member-title {
	position: absolute;
	top: 0;
	left: 0;
	right: 0;
}
.member-coupon-box {
	width: 100%;
	height: 100%;
	background-color: #fff;
	border-radius: 10px;
	overflow: hidden;
	position: relative;
	padding-top: 77px;
}
.member-coupon-header {
	position: absolute;
	width: 100%;
	top: 0;
	padding: 26px 10px 13px 10px;
	border-bottom: solid 1px #eb6100;
}
.member-coupon-body {
	padding-top: 20px;
	padding-bottom: 20px;
	width: 100%;
	height: 100%;
	overflow: hidden;
}
.coupon .mui-segmented-control {
	border: none;
	border-radius: 0;
	display: block;
	width: 100%;
	overflow: hidden;
}
.coupon .mui-control-item {
	display: inline-block;
	float: left;
	color: #969696;
	width: 44%;
	margin-left: 3%;
	margin-right: 3%;
	border: none;
}
.coupon .mui-control-item.mui-active {
	background: #fd8c3d;
	border-radius: 7px;
}
.personal-result-box {
	width: 100%;
	height: 100%;
	overflow: hidden;
}
.personal-list {
    height: 100%;
    overflow-y: auto;
}
.personal-list > div {
    margin-bottom: 5px;
}
.personal-list > div:last-child {
	margin-bottom: 0;
}
.personal-coupon {
    width: 100%;
    overflow: hidden;
    position: relative;
    overflow: hidden;
}
.personal-coupon img {
	display: block;
    height: 100px;
    min-width: 100%;
}
.personal-coupon .personal-coupon-box, .personal-coupon .personal-coupon-info {
    position: absolute;
    font-size: 10px;
}
.personal-coupon .personal-coupon-box {
    top: 5px;
    left: 20px;
    right: 90px;
    line-height: 1.5;
    color: #fff;
}
.personal-coupon .personal-coupon-box > * {
    color: #fff;
    font-size: 10px;
    margin-top: 10px;
    margin-bottom: 0;
    line-height: 1;
}
.personal-coupon .personal-coupon-info {
    top: 50%;
    right: 10px;
    width: 60px;
    transform: translateY(-50%);
    text-align: center;
}
.coupon-btn {
    display: block;
    background-color: #fff;
    color: #eb6100;
    margin-bottom: 5px;
}
.coupon-disable {
	background: #c8c8c8 !important;
	color: #fff !important;
}
.personal-coupon-info p {
    margin-bottom: 0;
    font-size: 10px;
    line-height: 1.5;
    color: #000;
}
.member-center {
	position: absolute;
	top: 50%;
	left: 50%;
	transform: translate(-50%, -50%);
}
.member-coupon-body .member-center {
	width: 100%;
	top: 40%;
	font-size: 15px;
	padding-left: 15px;
	padding-right: 15px;
	text-align: center;
}
.coupon .mui-popover {
	position: fixed;
	width: 80%;
	top: 50% !important;
	left: 50% !important;
	transform: translate(-50%, -50%);
	padding: 25px;
}
.coupon .mui-popover .mui-popover-arrow::after {
	height: 0;
}
.member-pop-coupon .member-flex {
	margin-bottom: 10px;
}
.member-pop-coupon .member-flex:last-child {
	margin-bottom: 0;
}
.member-pop-coupon label {
	font-size: 12px;
	font-weight: bold;
	white-space: nowrap;
	color: #646464;
}
.member-pop-coupon .member-flex-main p {
	font-size: 12px;
	color: #646464;
}

/*扫码支付*/
.QRcode .member {
	padding-top: 10px;
}
.QRcode .member-title {
	margin-top: 0;
}
.QRcode-box {
	position: absolute;
	top: 83px;
	right: 46px;
	left: 46px;
	bottom: 90px;
	background: #fff;
	border-radius: 10px;
	box-shadow: 0 0 12px 2px rgba(0,0,0,.16);
}
.QRcode-user {
	padding: 20px 15px 15px 15px;
	align-items: center;
	border-bottom: solid 1px #c8c8c8;
}
.QRcode-user-img {
	width: 40px;
	height: 40px;
	overflow: hidden;
	border-radius: 50%;
}
.QRcode-user-img > img {
	width: 100%;
	height: 100%;
}
.QRcode-user .member-flex-main {
	padding-left: 10px;
}
.QRcode-price {
	position: absolute;
	width: 100%;
	background-color: #f5f5fa;
	bottom: 0;
	text-align: center;
	padding-top: 15px;
	padding-bottom: 20px;
	border-radius: 0 0 10px 10px;
}
.QRcode-price h5 {
	font-size: 14px;
	color: #000;
}
.QRcode-price .member-btn-5 {
	margin-top: 10px;
}
.QRcode-img {
	position: absolute;
	width: 64%;
	max-width: 100px;
	top: 50%;
	left: 50%;
	margin-top: -8px;
	transform: translate(-50%, -50%);
}
.QRcode-img > img {
	width: 100%;
}

/*个人资料*/
.userInfo-header {
	width: 86px;
	height: 86px;
	border-radius: 6px;
	overflow: hidden;
}
.userInfo-header > img {
	width: 100%;
