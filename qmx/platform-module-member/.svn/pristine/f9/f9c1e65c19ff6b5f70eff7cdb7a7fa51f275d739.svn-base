/*门票预订*/
.reserve{
	border-top:solid 1px #cfcfcf;
	color:#737373;
}
.reserve .res_li{
	padding:10px 8px;
	border-bottom:solid 1px #cfcfcf;
	overflow:hidden;
	height:auto;
}
.reserve .res_li .res_pic{
	width:68px;
	height:68px;
	float:left;
	overflow:hidden;
}
.reserve .res_li .res_pic img{
	height:68px;
	min-width:68px;
}
.reserve .res_li .res_info{
	height:68px;
	text-align:left;
}
.reserve .res_li .res_info .res_inf_data{
	height:100%;
	position:relative;
	
}
.reserve .res_li .res_info .res_inf_data .inf_data_price{
	text-align:center;
	height:100%;
	position:relative;
	float:right;
}
