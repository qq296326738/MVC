/*语音画册*/
.voicealbum_box{
	box-sizing:border-box;
	padding:10px;
}
.voicealbum_box .voi_title{
	color:#fff;
	text-align:left;
	font-size:1.5rem;
}
.voicealbum_box .voi_title span{
	font-size:1rem;	
	margin-left:3px;
}
.voicealbum_box .voi_title .voi_tit_icon{
	display:inline-block;
	width:12px;
	height:15px;
	background:url(../images/voicealbum_icon.png) no-repeat scroll 0 0 / 12px 15px;
	vertical-align:-2px;
	margin-right:10px;
}
.voicealbum_box .voi_spot{
	margin-top:10px;
	color:#787878;
	overflow:hidden;
}
.voicealbum_box .voi_spot .voi_spo_left{
	width:45%;
	margin:20px 2%;
	overflow:hidden;
	display:inline-block;
	box-shadow:0px 0px 5px 1px #bcbcbc;
	vertical-align:top;
	position:relative;
	border:solid 1px transparent;
	border-radius:5px;
	float:left;
}
.voicealbum_box .voi_spot .voi_spo_left:nth-child(n+3){
	margin-top:0;	
}
.voicealbum_box .voi_spot .voi_spo_right{
	max-width:159px;
	width:50%;
	overflow:hidden;
	display:inline-block;
	margin-left:5px;
	box-shadow:0px 0px 10px 1px #444;	
}
.voicealbum_box .voi_spot .voi_spo_right .spo_rig_sha{
	width:159px;
	max-height:200px;
	overflow-y:scroll;
}
.voicealbum_box .voi_spot .voi_spo_right .spo_rig_li{
	width:100%;
	height:50px;
	background-color:#fff;
	box-sizing:border-box;
	border-bottom:solid 1px #ededed;
	padding:6px;
	position:relative;
}
.voicealbum_box .voi_spot .voi_spo_right .spo_rig_li:last-child{
	border:none;
}
.voicealbum_box .voi_spot .spo_lef_pic{
	min-width:100%;
	height:500px;
	display:block;
}
.voicealbum_box .voi_spot .spo_lef_title{
	position:relative;
	width:100%;
	background-color:#fff;
	font-weight:100;
	padding:20px 0 10px 0;
	font-size:1rem;
}
.voicealbum_box .voi_spot .voi_spo_right .spo_rig_li .spo_rig_pic{
	width:48px;
	height:38px;
	float:left;
}
.voicealbum_box .voi_spot .voi_spo_right .spo_rig_li .spo_rig_name{
	height:38px;
	line-height:38px;
	font-size:0.8rem;
	text-indent:10px;
	overflow:hidden;
}
		/*声音按钮*/
.audio_button_1,.audio_button_2{
	display:block;
	position:absolute;
}
.audio_button_1{
	width:28px;
	height:28px;
	border:solid 1px #fff;
	border-radius:50%;
	top:-14px;
	/*left:47px;*/
	left:50%;
	margin-left:-14px;
	box-shadow:0 0 3px 1px #999;
	background:url(../images/voicealbum_audio.png) no-repeat scroll #fff center center / 17px 15px;
}
.audio_button_2{
	width:22px;
	height:22px;
