/*首页*/
.supplier{
	width:100%;
	height:auto;
	background:#4dd0c8;
	padding:36px 0 18px 0;
}
.logo{
	width:108px;
	height:108px;
	border:solid 1px #fff;
	border-radius:50%;
	margin:0 auto;
	background:#fff;
	text-align:center;
	position:relative;
	overflow:hidden;
}
