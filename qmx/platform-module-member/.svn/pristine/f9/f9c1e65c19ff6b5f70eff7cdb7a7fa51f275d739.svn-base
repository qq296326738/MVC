		/*轮播start*/
.slide{
	position:relative;
	width:100%;
	overflow:hidden;
}
.slide .slide-imgul{
	width:10000px;
}
.slide .slide-imgul li{
	float:left;
}
.slide .slide-imgul li a{
	display:block;
}
.slide .slide-imgul li img{
	width:100%;
}
.slide .slide-control{
	position:absolute;
	bottom:10px;
	right:10px;
}
.conter-ul li{
	float:left;
	margin:0 3px;
}
.slide .slide-control .conter-ul li{
	width:8px;
	height:8px;
	border:solid 1px;
	border-color:rgba(255,255,255,0.8);
	background-color:rgba(255,255,255,0.8);
	margin:0 2px;
	border-radius: 5px;
}
.slide .slide-control .conter-ul li.picth{
	background-color:#7cddd7;
	border-color:#7cddd7;
}
		/*轮播end*/
