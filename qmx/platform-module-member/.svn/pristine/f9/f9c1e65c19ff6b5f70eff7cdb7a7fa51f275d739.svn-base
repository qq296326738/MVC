	border:solid 1px #4dd0c8;
	padding:3px 5px;
	display:block;
	bottom:0;
	position:absolute;
	left:50%;
	margin-left:-19px;
	border-radius:5px;
}
.inf_dat_title,.inf_dat_number{
	position:absolute;
	font-size:0.8rem;
	left:68px;
	right:20%;
	box-sizing:border-box;
	padding-left:8px;
	line-height:1.2;
}
.reserve .res_li .res_info .res_inf_data .inf_dat_number{
	bottom:0;
}
