/*资源方*/
*{
	margin:0;
	padding:0;
}

a{
	text-decoration:none;	
}

ul{
	list-style:none;
}

html{
	font-size:16px;
	font-family:"黑体";
}

th{
	font-weight:100;
}
a,a:link,a:visited,a:hover,a:active{
	color:#000;
}
ul,li{
	list-style:none;	
}

/*公共*/
.box{
	width:100%;
	height:auto;
	text-align:center;
}
.borbox-3{
	width:33.333%;
