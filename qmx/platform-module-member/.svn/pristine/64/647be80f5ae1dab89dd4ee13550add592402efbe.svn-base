package com.qmx.member.query;

import com.qmx.base.api.dto.QueryDto;
import com.qmx.member.enumerate.InviteType;

/**
 * @Author liubin
 * @Description 接口日志查询
 * @Date Created on 2018/7/4 17:21.
 * @Modified By
 */
public class GdsInviteVO extends QueryDto {

    private Long id;

    /**
     * 邀请人id
     */
    private Long memberId;
    /**
     * 被邀人openid
     */
    private String openid;

    /**
     * 被邀人id
     */
    private Long byId;

    /**
     * 被邀人是否关注公众号
     */
    private Boolean state;

    /**
     * 分享赠送积分
     */
    private Integer codeIntegral;

    /**
     * 分享赠送积分
     */
    private Double codeMoney;
    /**
     * 分享类型
     */
    private InviteType inviteType;

    public Long getById() {
        return byId;
    }

    public void setById(Long byId) {
        this.byId = byId;
    }

    public Double getCodeMoney() {
        return codeMoney;
    }

    public void setCodeMoney(Double codeMoney) {
        this.codeMoney = codeMoney;
    }

    public InviteType getInviteType() {
        return inviteType;
    }

    public void setInviteType(InviteType inviteType) {
        this.inviteType = inviteType;
    }

    public Long getMemberId() {
        return memberId;
    }

    public void setMemberId(Long memberId) {
        this.memberId = memberId;
    }

    public String getOpenid() {
        return openid;
    }

    public void setOpenid(String openid) {
        this.openid = openid;
    }

    public Boolean getState() {
        return state;
    }

    public void setState(Boolean state) {
        this.state = state;
    }

    public Long getId() {
        return id;
    }

    public void setId(Long id) {
        this.id = id;
    }

    public Integer getCodeIntegral() {
        return codeIntegral;
    }

    public void setCodeIntegral(Integer codeIntegral) {
        this.codeIntegral = codeIntegral;
    }
}